#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/aluno/Documentos/2020-clock/node_modules/css-prefers-color-scheme/cli.js/node_modules:/home/aluno/Documentos/2020-clock/node_modules/css-prefers-color-scheme/node_modules:/home/aluno/Documentos/2020-clock/node_modules:/home/aluno/Documentos/node_modules:/home/aluno/node_modules:/home/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../css-prefers-color-scheme/cli.js" "$@"
else 
  exec node  "$basedir/../css-prefers-color-scheme/cli.js" "$@"
fi
