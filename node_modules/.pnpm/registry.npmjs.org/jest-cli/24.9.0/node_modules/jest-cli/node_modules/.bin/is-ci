#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/is-ci/2.0.0/node_modules/is-ci/bin.js/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/is-ci/2.0.0/node_modules/is-ci/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/is-ci/2.0.0/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/is-ci/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/node_modules:/home/aluno/Documentos/2020-clock/node_modules:/home/aluno/Documentos/node_modules:/home/aluno/node_modules:/home/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../is-ci/2.0.0/node_modules/is-ci/bin.js" "$@"
else 
  exec node  "$basedir/../../../../../../is-ci/2.0.0/node_modules/is-ci/bin.js" "$@"
fi
