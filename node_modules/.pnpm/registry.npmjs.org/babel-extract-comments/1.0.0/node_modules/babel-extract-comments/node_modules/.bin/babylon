#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/babylon/6.18.0/node_modules/babylon/bin/babylon.js/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/babylon/6.18.0/node_modules/babylon/bin/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/babylon/6.18.0/node_modules/babylon/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/babylon/6.18.0/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/babylon/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/node_modules:/home/aluno/Documentos/2020-clock/node_modules:/home/aluno/Documentos/node_modules:/home/aluno/node_modules:/home/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../babylon/6.18.0/node_modules/babylon/bin/babylon.js" "$@"
else 
  exec node  "$basedir/../../../../../../babylon/6.18.0/node_modules/babylon/bin/babylon.js" "$@"
fi
