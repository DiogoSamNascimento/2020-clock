#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/webpack-dev-server/3.10.2_webpack@4.41.5/node_modules/webpack-dev-server/bin/webpack-dev-server.js/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/webpack-dev-server/3.10.2_webpack@4.41.5/node_modules/webpack-dev-server/bin/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/webpack-dev-server/3.10.2_webpack@4.41.5/node_modules/webpack-dev-server/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/webpack-dev-server/3.10.2_webpack@4.41.5/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/webpack-dev-server/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/node_modules:/home/aluno/Documentos/2020-clock/node_modules:/home/aluno/Documentos/node_modules:/home/aluno/node_modules:/home/node_modules:/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/node_modules/webpack-dev-server/bin/webpack-dev-server.js/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/node_modules/webpack-dev-server/bin/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/node_modules/webpack-dev-server/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../webpack-dev-server/bin/webpack-dev-server.js" "$@"
else 
  exec node  "$basedir/../webpack-dev-server/bin/webpack-dev-server.js" "$@"
fi
