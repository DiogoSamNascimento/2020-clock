#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/@cnakazawa/watch/1.0.4/node_modules/@cnakazawa/watch/cli.js/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/@cnakazawa/watch/1.0.4/node_modules/@cnakazawa/watch/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/@cnakazawa/watch/1.0.4/node_modules/@cnakazawa/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/@cnakazawa/watch/1.0.4/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/@cnakazawa/watch/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/@cnakazawa/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/node_modules:/home/aluno/Documentos/2020-clock/node_modules:/home/aluno/Documentos/node_modules:/home/aluno/node_modules:/home/node_modules:/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/node_modules/@cnakazawa/watch/cli.js/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/node_modules/@cnakazawa/watch/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/node_modules/@cnakazawa/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@cnakazawa/watch/cli.js" "$@"
else 
  exec node  "$basedir/../@cnakazawa/watch/cli.js" "$@"
fi
