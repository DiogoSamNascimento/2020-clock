#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/css-blank-pseudo/0.1.4/node_modules/css-blank-pseudo/cli.js/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/css-blank-pseudo/0.1.4/node_modules/css-blank-pseudo/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/css-blank-pseudo/0.1.4/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/css-blank-pseudo/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/node_modules:/home/aluno/Documentos/2020-clock/node_modules:/home/aluno/Documentos/node_modules:/home/aluno/node_modules:/home/node_modules:/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/node_modules/css-blank-pseudo/cli.js/node_modules:/home/aluno/Documentos/2020-clock/node_modules/.pnpm/node_modules/css-blank-pseudo/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../css-blank-pseudo/cli.js" "$@"
else 
  exec node  "$basedir/../css-blank-pseudo/cli.js" "$@"
fi
